@page
@model phr.Pages.Exceptions.General_Information.WelldownInfoModel
@{
	ViewData["Title"] = "Welldown Info";
}
<div class="container p-2 d-flex align-items-end flex-column vh-100">
	<div class="col-12 py-2 border-bottom">
		<h4><strong>Welldown Info</strong></h4>
	</div>
	<div class="col-12">
		<label for="down-date" class="form-label mt-3"><strong>Down Date</strong></label>
		<input class="form-control" id="down-date" type="text" value="20-May-2024" readonly disabled>

		<label for="s-or-ss" class="form-label mt-3"><strong>S or SS</strong></label>
		<input class="form-control" id="s-or-ss" type="text" value="Sub-Surface" readonly disabled>

		<label for="hrs-since-off" class="form-label mt-3"><strong>HRS Since Off</strong></label>
		<input class="form-control" id="hrs-since-off" type="text" value="316.33" readonly disabled>

		<label for="proact-react" class="form-label mt-3"><strong>Proact. or React</strong></label>
		<input class="form-control" id="proact-react" type="text" value="Reactive" readonly disabled>

		<label for="days-since-off" class="form-label mt-3"><strong>Days Since Off</strong></label>
		<input class="form-control" id="days-since-off" type="text" value="13.18" readonly disabled>

		<label for="event-category" class="form-label mt-3"><strong>Event Category</strong></label>
		<input class="form-control" id="event-category" type="text" value="RESERVOIR_56" readonly disabled>

		<label for="bopd-deferred" class="form-label mt-3"><strong>BOPD Deferred</strong></label>
		<input class="form-control" id="bopd-deferred" type="text" value="4.87" readonly disabled>

		<label for="event-reason" class="form-label mt-3"><strong>Event Reason</strong></label>
		<input class="form-control" id="event-reason" type="text" value="INJECTIVITY" readonly disabled>

		<label for="total-down-oil" class="form-label mt-3"><strong>Total Down Oil</strong></label>
		<input class="form-control" id="total-down-oil" type="text" value="64.19" readonly disabled>

		<label for="event-system" class="form-label mt-3"><strong>Event System</strong></label>
		<input class="form-control" id="event-system" type="text" value="SS_RESERVOIR" readonly disabled>

		<label for="remark" class="form-label mt-3"><strong>Remark</strong></label>
		<input class="form-control" id="remark" type="text" value="Shut off due to MSTL" readonly disabled>

	</div>
</div>
